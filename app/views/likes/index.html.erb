<%= render "shared/header" %>

<div class='main'>

  <%# Left-side %>
  <div class='top-left-content'>   
    <div class='whose-item-list-navi'>
      <%= image_tag @liked_user.profile.image.variant(resize_to_fill: [128, 128]), class: 'whose-user-avatar'%>
      <p><%= @liked_user.username%> 's Like</p>
    </div>
    <%# @items リスト展開 %>
    <div id='item-pagenate'>
      <%= render "shared/item-list" %>
    </div>
    <%= link_to_next_page @items, 'もっと見る', remote: true, class: 'more-link', id: 'more-link' %>
    <%# // @items リスト展開 %>   
  </div>
  <%# // Left-side%>

  
  <%# Right-side %>
  <%= render 'shared/right_side' %>
  <%# // Right-side %>

</div>

<%# スマホ用コマンド %>
<%= render 'shared/command'%>
<%# // スマホ用コマンド %>

<%= render "shared/footer" %>